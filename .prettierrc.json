{
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"requirePragma": false,
	"insertPragma": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "avoid",
	"rangeStart": 0,
	"proseWrap": "preserve",
	"parser": "typescript"
}
